PROCESS BEFORE OUTPUT.
*&SPWIZARD: PBO FLOW LOGIC FOR TABLECONTROL 'TC_EL'
  MODULE TC_EL_INIT.
*&SPWIZARD: MODULE TC_EL_CHANGE_TC_ATTR.
*&SPWIZARD: MODULE TC_EL_CHANGE_COL_ATTR.
  LOOP AT   G_TC_EL_ITAB
       INTO G_TC_EL_WA
       WITH CONTROL TC_EL
       CURSOR TC_EL-CURRENT_LINE.
*&SPWIZARD:   MODULE TC_EL_CHANGE_FIELD_ATTR
    MODULE TC_EL_MOVE.
    MODULE TC_EL_GET_LINES.
  ENDLOOP.
CALL SUBSCREEN SSA INCLUDING 'ZAG_OS_TOTAL_SALES1' '2100'.
 MODULE STATUS_1000.
   MODULE SHOW_LOGO.
*
PROCESS AFTER INPUT.
*&SPWIZARD: PAI FLOW LOGIC FOR TABLECONTROL 'TC_EL'
  LOOP AT G_TC_EL_ITAB.
    CHAIN.
      FIELD ZAG_OS_BILL-PID.
      FIELD ZAG_OS_BILL-PNAME.
      FIELD ZAG_OS_BILL-PRICE.
      FIELD ZAG_OS_BILL-QTY.
      FIELD ZAG_OS_BILL-AMOUNT.
    ENDCHAIN.
  ENDLOOP.
  MODULE TC_EL_USER_COMMAND.
*&SPWIZARD: MODULE TC_EL_CHANGE_TC_ATTR.
*&SPWIZARD: MODULE TC_EL_CHANGE_COL_ATTR.
CALL SUBSCREEN SSA.
 MODULE USER_COMMAND_1000.
